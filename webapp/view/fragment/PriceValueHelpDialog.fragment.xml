<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>ttlPrice}"
		ok="onPriceValueHelpDialogOkPress"
		cancel="onPriceValueHelpDialogCancelPress"
		key="id"
		descriptionKey="price"
		afterClose="onPriceValueHelpDialogAfterClose"
		supportRanges="true"
		supportRangesOnly="true"
		maxIncludeRanges="1"
		maxExcludeRanges="0"
		maxConditions="1"/>
</core:FragmentDefinition>